\"react\";\nimport { Provider } from \"react-redux\";\nimport { configureStore, combineReducers } from \"@reduxjs/toolkit\";\nimport { BrowserRouter, Switch, Route } from \"react-router-dom\";\n\n// 1. added import form\nimport form from \"./reducers/form\";\nimport user from \"./reducers/user\";\n\nimport Home from \"./components/pages/Home\";\nimport PetPosts from \"./components/pages/PetPosts\";\nimport PetPostDetails from \"./components/pages/PetPostDetails\";\nimport About from \"./components/pages/About\";\nimport PostForm from \"./components/PostForm\";\nimport Login from \"./components/pages/Login\";\nimport Register from \"./components/pages/Register\";\nimport Navbar from './components/Navbar';\nimport TestNav from \"./components/WIP/TestNav\";\nimport Footer from \"./components/Footer\";\n\n// 2. added form reducer in combineReducers\nconst reducer = combineReducers({\n  user: user.reducer,\n  form: form.reducer,\n});\n\nconst store = configureStore({ reducer });\n\nconst App = () => {\n  return (\n      <Provider store={store}>\n        <BrowserRouter>\n          {/* <TestNav /> */}\n          <Navbar />\n          <Switch>\n            <Route exact path=\"/\" component={Home} />\n            <Route path=\"/petposts\" component={PetPosts} />\n            <Route path=\"/pet/:id\" component={PetPostDetails} />\n            <Route path=\"/create-post\" component={PostForm} />\n            <Route path=\"/about\" component={About} />\n            <Route path=\"/register\" component={Register} />\n            <Route path=\"/login\" component={Login} />\n          </Switch>\n          <Footer />\n        </BrowserRouter>\n      </Provider>\n  );\n};\n\nexport default App;\n","/home/coffeedoodles/Technigo/final-project-fe/petspotter-fe/code/src/reducers/form.js",[],"/home/coffeedoodles/Technigo/final-project-fe/petspotter-fe/code/src/reducers/user.js",[],"/home/coffeedoodles/Technigo/final-project-fe/petspotter-fe/code/src/components/pages/Home.js",[],"/home/coffeedoodles/Technigo/final-project-fe/petspotter-fe/code/src/components/pages/About.js",[],"/home/coffeedoodles/Technigo/final-project-fe/petspotter-fe/code/src/components/pages/PetPosts.js",[],"/home/coffeedoodles/Technigo/final-project-fe/petspotter-fe/code/src/components/Navbar.js",[],"/home/coffeedoodles/Technigo/final-project-fe/petspotter-fe/code/src/components/pages/Login.js",[],"/home/coffeedoodles/Technigo/final-project-fe/petspotter-fe/code/src/components/pages/Register.js",[],"/home/coffeedoodles/Technigo/final-project-fe/petspotter-fe/code/src/components/PostForm.js",["111"],"import React, { useState, useRef } from \"react\";\nimport { useSelector } from \"react-redux\";\nimport { useHistory } from \"react-router-dom\";\n\nimport { REACT_APP_BASE_URL } from \"../reuseables/urls\";\nimport { MainContainer, Form } from \"./styled-components/FormElements\";\n\nconst IMAGE_API_URL = `https://pet-spotter.herokuapp.com/upload-images`;\n\n\nconst PostForm = () => {\n  const [status, setStatus] = useState(\"\");\n  const [petName, setPetName] = useState(\"\");\n  const [species, setSpecies] = useState(\"\");\n  const [breed, setBreed] = useState(\"\");\n  const [sex, setSex] = useState(\"\");\n  const [location, setLocation] = useState(\"\");\n  const [description, setDescription] = useState(\"\");\n  const [email, setEmail] = useState(\"\");\n\n  const accessToken = useSelector((store) => store.user.accessToken);\n  const history = useHistory();\n  const fileInput = useRef();\n\n  const handleOnSubmit = (e) => {\n    e.preventDefault();\n\n    const formData = new FormData();\n    formData.append(\"image\", fileInput.current.files[0]);\n\n    fetch(IMAGE_API_URL, { method: \"POST\", body: formData })\n      .then((res) => res.json())\n      .then(({ imageUrl }) => {\n        fetch(`https://pet-spotter.herokuapp.com/petposts`, {\n          method: \"POST\",\n          headers: {\n            \"Content-Type\": \"application/json\",\n            Authorization: accessToken,\n          },\n          body: JSON.stringify({\n            status,\n            petName,\n            species,\n            breed,\n            sex,\n            location,\n            description,\n            email,\n            imageUrl,\n          }),\n        }).then(() => {\n          history.push(\"/petposts\");\n        });\n      });\n  };\n\n  return (\n    <MainContainer className=\"create-post\">\n      <h2>Create a new post!</h2>\n      <Form onSubmit={handleOnSubmit}>\n        <div className=\"radio-wrapper\">\n          <label htmlFor=\"status\">Pet Status:</label>\n          <input\n            id=\"statusLost\"\n            name=\"statusRadio\"\n            type=\"radio\"\n            required\n            value=\"lost\"\n            onChange={(e) => setStatus(e.target.value)}\n          />\n          <label htmlFor=\"status\">Lost</label>\n          <input\n            id=\"statusFound\"\n            name=\"statusRadio\"\n            type=\"radio\"\n            required\n            value=\"found\"\n            onChange={(e) => setStatus(e.target.value)}\n          />\n          <label htmlFor=\"status\">Found</label>\n          <input\n            id=\"statusReturned\"\n            name=\"statusRadio\"\n            type=\"radio\"\n            required\n            value=\"returned\"\n            onChange={(e) => setStatus(e.target.value)}\n          />\n          <label htmlFor=\"returned-home\">Returned Home</label>\n        </div>\n        <div>\n          <label htmlFor=\"petImage\">\n            Pet Image\n            <input type=\"file\" ref={fileInput} />\n          </label>\n        </div>\n        <label htmlFor=\"petName\">Pet Name:</label>\n        <input\n          id=\"petName\"\n          type=\"text\"\n          value={petName}\n          onChange={(e) => setPetName(e.target.value)}\n        />\n        <label htmlFor=\"species\">Species:</label>\n        <select\n          id=\"species\"\n          value={species}\n          onChange={(e) => setSpecies(e.target.value)}\n        >\n          <option defaultValue>Select an Species (More coming soon!)</option>\n          <option value=\"Dog\">Dog</option>\n          <option value=\"Cat\">Cat</option>\n        </select>\n        <label htmlFor=\"breed\">Breed:</label>\n        <input\n          id=\"breed\"\n          type=\"text\"\n          required\n          value={breed}\n          onChange={(e) => setBreed(e.target.value)}\n        />\n        <div className=\"radio-wrapper\">\n          <label htmlFor=\"sex\">Sex:</label>\n          <input\n            id=\"sexFemale\"\n            name=\"sexRadio\"\n            type=\"radio\"\n            required\n            value=\"female\"\n            onChange={(e) => setSex(e.target.value)}\n          />\n          <label htmlFor=\"sex\">Female</label>\n          <input\n            id=\"sexMale\"\n            name=\"sexRadio\"\n            type=\"radio\"\n            required\n            value=\"male\"\n            onChange={(e) => setSex(e.target.value)}\n          />\n          <label htmlFor=\"sex\">Male</label>\n          <input\n            id=\"sexOther\"\n            name=\"sexRadio\"\n            type=\"radio\"\n            required\n            value=\"other\"\n            onChange={(e) => setSex(e.target.value)}\n          />\n          <label htmlFor=\"other\">Other</label>\n        </div>\n        <label htmlFor=\"location\">Location:</label>\n        <input\n          id=\"location\"\n          type=\"text\"\n          required\n          value={location}\n          onChange={(e) => setLocation(e.target.value)}\n        />\n        <label htmlFor=\"description\">Description:</label>\n        <textarea\n          id=\"description\"\n          placeholder=\"Please describe in as much detail as possible.\n           Fur color/patterns, eye color, distinguishing marks etc.\"\n          required\n          value={description}\n          onChange={(e) => setDescription(e.target.value)}\n        ></textarea>\n        <label htmlFor=\"email\">Contact Email:</label>\n        <input\n          id=\"email\"\n          type=\"email\"\n          required\n          value={email}\n          onChange={(e) => setEmail(e.target.value)}\n        />\n        <button type=\"submit\">Add Post</button>\n        <button>Cancel</button>\n      </Form>\n    </MainContainer>\n  );\n};\n\nexport default PostForm;\n","/home/coffeedoodles/Technigo/final-project-fe/petspotter-fe/code/src/components/cards/CardList.js",[],"/home/coffeedoodles/Technigo/final-project-fe/petspotter-fe/code/src/reuseables/urls.js",[],"/home/coffeedoodles/Technigo/final-project-fe/petspotter-fe/code/src/components/Footer.js",[],"/home/coffeedoodles/Technigo/final-project-fe/petspotter-fe/code/src/components/HeroSection.js",[],"/home/coffeedoodles/Technigo/final-project-fe/petspotter-fe/code/src/components/ArticleSection.js",[],"/home/coffeedoodles/Technigo/final-project-fe/petspotter-fe/code/src/components/styled-components/FormElements.js",[],"/home/coffeedoodles/Technigo/final-project-fe/petspotter-fe/code/src/components/styled-components/NavbarElements.js",[],["112","113"],"/home/coffeedoodles/Technigo/final-project-fe/petspotter-fe/code/src/components/styled-components/CardElements.js",[],"/home/coffeedoodles/Technigo/final-project-fe/petspotter-fe/code/src/components/styled-components/FooterElements.js",[],"/home/coffeedoodles/Technigo/final-project-fe/petspotter-fe/code/src/components/pages/PetPostDetails.js",["114"],"import React, { useState, useEffect } from 'react'\nimport { useParams } from 'react-router-dom';\nimport styled from 'styled-components/macro';\n\nimport { REACT_APP_BASE_URL } from '../../reuseables/urls';\n\n\n\nconst PetPostDetails = () => {\n  const { id } = useParams();\n  const [details, setDetails] = useState({});\n\n  const PET_DETAILS_URL = `${REACT_APP_BASE_URL}/${id}`\n \n  useEffect(() => {\n    fetch(PET_DETAILS_URL)\n      .then((res) => res.json())\n   